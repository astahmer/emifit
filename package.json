{
    "name": "emifit",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "dev:sw": "SW_DEV=true vite",
        "local": "vite --host --port 3001",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "prettier": "prettier --write \"./{src,tests}/**/*\"",
        "release": "esno ./release.mts && vercel && git push"
    },
    "dependencies": {
        "@babel/core": "^7.18.2",
        "@chakra-ui/icons": "^1.1.7",
        "@chakra-ui/react": "^1.8.8",
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@pastable/core": "^0.1.19",
        "@uselessdev/datepicker": "^2.4.1",
        "@xstate/react": "^2.0.1",
        "browser-fs-access": "^0.25.3",
        "canvas-confetti": "^1.5.1",
        "chakra-color-picker": "^0.0.6",
        "click-to-react-component": "^1.0.8",
        "date-fns": "^2.28.0",
        "downshift": "^6.1.7",
        "framer-motion": "^6.3.11",
        "history": "^5.3.0",
        "idb": "^7.0.1",
        "idb-keyval": "^6.1.0",
        "jotai": "^1.7.2",
        "nanoid": "^3.3.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-hook-form": "^7.32.0",
        "react-hotkeys-hook": "^3.4.6",
        "react-icons": "^4.4.0",
        "react-query": "^4.0.0-beta.23",
        "react-router-dom": "^6.3.0",
        "react-table": "^7.8.0",
        "react-virtual": "^2.10.4",
        "ts-pattern": "^4.0.3",
        "valtio": "^1.6.1",
        "xstate": "^4.32.1"
    },
    "devDependencies": {
        "@rollup/plugin-replace": "^4.0.0",
        "@types/canvas-confetti": "^1.4.2",
        "@types/react": "^17.0.45",
        "@types/react-dom": "^17.0.17",
        "@types/react-table": "^7.7.12",
        "@vitejs/plugin-react": "^1.3.2",
        "axios": "^0.25.0",
        "dotenv": "^16.0.1",
        "esno": "^0.16.3",
        "prettier": "^2.6.2",
        "prettier-plugin-sorted": "^2.0.0",
        "ts-toolbelt": "^9.6.0",
        "typescript": "^4.7.3",
        "vite": "^2.9.12",
        "vite-plugin-checker": "^0.4.6",
        "vite-plugin-pwa": "^0.11.13",
        "workbox-core": "^6.5.3",
        "workbox-precaching": "^6.5.3",
        "workbox-routing": "^6.5.3",
        "workbox-window": "^6.5.3"
    },
    "importSort": {
        ".js, jsx, .ts, .tsx": {
            "options": {
                "ignoreTsConfig": true,
                "extraAliases": [
                    "@/*"
                ],
                "bottomAliases": [
                    "/assets"
                ]
            }
        }
    }
}
