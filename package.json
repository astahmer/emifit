{
    "name": "emifit",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --host",
        "dev:sw": "SW_DEV=true vite",
        "local": "vite --host --port 3001",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "prettier": "prettier --write \"./{src,tests}/**/*\"",
        "release": "esno ./release.mts && vercel --prod && git push"
    },
    "dependencies": {
        "@babel/core": "^7.18.9",
        "@chakra-ui/icons": "^2.0.4",
        "@chakra-ui/react": "^2.2.4",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@tanstack/react-query": "^4.0.9",
        "@tanstack/react-query-devtools": "^4.0.8",
        "@uselessdev/datepicker": "^2.5.1",
        "@xstate/react": "^3.0.0",
        "browser-fs-access": "^0.25.3",
        "canvas-confetti": "^1.5.1",
        "chakra-color-picker": "^0.0.6",
        "click-to-react-component": "^1.0.8",
        "date-fns": "^2.28.0",
        "downshift": "^6.1.7",
        "framer-motion": "^6.5.1",
        "history": "^5.3.0",
        "idb": "^7.0.2",
        "idb-keyval": "^6.2.0",
        "jotai": "^1.7.6",
        "nanoid": "^3.3.4",
        "pastable": "^2.0.10",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-error-boundary": "^3.1.4",
        "react-hook-form": "^7.33.0",
        "react-hotkeys-hook": "^3.4.6",
        "react-icons": "^4.4.0",
        "react-router-dom": "^6.3.0",
        "react-swipeable": "^7.0.0",
        "react-table": "^7.8.0",
        "react-textarea-autosize": "^8.3.4",
        "react-use": "^17.4.0",
        "react-virtual": "^2.10.4",
        "recharts": "^2.1.12",
        "ts-pattern": "^4.0.5",
        "valtio": "^1.6.3",
        "xstate": "^4.32.1"
    },
    "devDependencies": {
        "@rollup/plugin-replace": "^4.0.0",
        "@types/canvas-confetti": "^1.4.3",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@types/react-table": "^7.7.12",
        "@vitejs/plugin-react": "^2.0.0",
        "axios": "^0.25.0",
        "dotenv": "^16.0.1",
        "esno": "^0.16.3",
        "prettier": "^2.7.1",
        "prettier-plugin-sorted": "^2.0.0",
        "ts-toolbelt": "^9.6.0",
        "typescript": "^4.7.4",
        "vite": "^3.0.2",
        "vite-plugin-checker": "^0.4.9",
        "vite-plugin-pwa": "^0.11.13",
        "vite-plugin-qrcode": "^0.1.1",
        "workbox-core": "^6.5.3",
        "workbox-precaching": "^6.5.3",
        "workbox-routing": "^6.5.3",
        "workbox-window": "^6.5.3"
    },
    "importSort": {
        ".js, jsx, .ts, .tsx": {
            "options": {
                "ignoreTsConfig": true,
                "extraAliases": [
                    "@/*"
                ],
                "bottomAliases": [
                    "/assets"
                ]
            }
        }
    }
}
